{
  "name": "npm-test-build-mb",
  "version": "0.0.0-beta.7",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "author": "",
  "license": "ISC",
  "scripts": {
    "analyze": "ANALYZE=1 next build",
    "storybook": "start-storybook -s ./public -p 9090",
    "build-storybook": "build-storybook -c .storybook -s ./public -o built-storybook",
    "storybook-production": "node .storybook/server.js",
    "lint": "eslint 'src/**/*.js?(x)'",
    "build": "rimraf dist && NODE_ENV=production BABEL_ENV=production rollup -c",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "react": "^17.x",
    "react-dom": "^17.x"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.5",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.5",
    "@babel/register": "^7.10.5",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/addon-knobs": "^5.3.19",
    "@storybook/addon-viewport": "^5.3.19",
    "@storybook/react": "^6.1.18",
    "airtable": "^0.11.1",
    "antd": "^4.17.3",
    "autoprefixer": "^10.4.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-css-modules-transform": "^1.6.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "css-modules-require-hook": "^4.2.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.6.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.5",
    "less": "^3.12.2",
    "less-loader": "4.1.0",
    "less-plugin-npm-import": "^2.1.0",
    "postcss": "^8.4.5",
    "pre-git": "^3.17.1",
    "react-intl": "^5.23.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-typescript2": "^0.31.1",
    "typescript": "^4.5.4"
  },
  "config": {
    "pre-git": {
      "enabled": true,
      "pre-commit": [
        "npm run test",
        "npm run lint"
      ],
      "pre-push": [],
      "post-commit": [],
      "post-checkout": [],
      "post-merge": []
    }
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  }
}
